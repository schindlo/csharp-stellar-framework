<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <PackageVersion>1.0.4</PackageVersion>
    <Authors>Oliver Schindler</Authors>
    <Description>.NET Standard 2.0 Stellar SDK</Description>
    <PackOnBuild>true</PackOnBuild>
    <PackageId>csharp-stellar-sdk</PackageId>
    <RootNamespace>StellarSdk</RootNamespace>
    <PackageProjectUrl>https://github.com/schindlo/csharp-stellar-framework</PackageProjectUrl>
  </PropertyGroup>

  <ItemGroup>
    <Folder Include="Model\" />
    <Folder Include="Exceptions\" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="10.0.3" />
  </ItemGroup>
</Project>
